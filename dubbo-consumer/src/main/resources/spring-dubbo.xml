<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="dubbo-consumer">
		<dubbo:parameter key="qos.enable" value="false" />
	</dubbo:application>


	<dubbo:registry protocol="nacos" address="127.0.0.1:8848" />

	<dubbo:protocol name="dubbo" port="-1" server="netty"
		client="netty" charset="UTF-8" threadpool="fixed" threads="500"
		queues="0" buffer="8192" accepts="0" payload="8388608" />

	<dubbo:reference timeout="50000"
		interface="com.fcc.api.service.DemoService" id="demoService" version="1.0.0"
		retries="0" check="false" actives="20" />
</beans>